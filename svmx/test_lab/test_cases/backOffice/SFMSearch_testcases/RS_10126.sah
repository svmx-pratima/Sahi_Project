/*
 * This test case verifies the quick save of SFM search and checking the SFM Search Delivery
 */
var $RS_10126 = _testcase("RS_10126", "quick save and checking the delivery");
$RS_10126.start();

// To generate Random names for SearchTitle and Search ID
var $searchTitle = $bo_act_common.generateRandomName("Search");
var $searchId = $bo_act_common.generateRandomName("SearchId");

var $data = _readExcelFile("../../backOffice/bo_excleData/BORegressionDatasheet.xlsx",$orgtype,true);
var $createwoquery = $data[1]["CreateWorkOrderOpen"];
var $fetchwo = $data[1]["fetchwo"];

// create a Work Order
$bo_act_login.login($g_url, $bac_un, $bac_pwd);

var $rs_10126_result = $op_act_sForceApiCall.sForceQuery($bac_un, $bac_pwd, $createwoquery ,"create");
var $rs_10126_query_wo = $fetchwo;
var $rs_10126_fetch = $op_act_sForceApiCall.sForceQuery($bac_un, $bac_pwd, $rs_10126_query_wo ,"query");

$rs_10126_WO1 = $rs_10126_fetch[0].Name;

// To create a Custom SFM Search value
// createsfmsearch = For Single Expression
$bo_act_sfmsearch.sfmsearch_advancedexpression($searchId, $searchTitle, "Work Order", "Priority", "High", "Equals", "Order Status", "Open", "10126");
$bo_act_sfmsearchdelivery.sfmsearch($searchTitle, $rs_10126_WO1, false);

$RS_10126.end();
