_assertContainsText("$122.000",$actualTotalBillableAmountOnWorkOrder,"Actual and expected Total Billable Amount On Work Order are not same");
_setSpeed($g_stepSpeed);
//Delete SFM Wizard 
$bo_act_SFW.deleteSFMWizard("Work Order", $wizardIDName);

