var $un = $cred[2]["UserName"];
var $pwd =$cred[2]["Password"];
var $url =$cred[2]["url"];
var $user =$cred[2]["User"];
var $data = _readExcelFile("../../analytics/an_excleData/MTTC_data.xlsx","Sheet1",true);
