//<browser>
function login($g_un,$g_pwd){
	
	var $login_result = sforce.connection.login($g_un,$g_pwd);
	return $login_result;
}

function create_WO ($wo_type , $wo_status, $wo_state)
{
	var workorder = new sforce.SObject("SVMXC__Service_Order__c");
	workorder.SVMXC__Order_Type__c = $wo_type;
	workorder.SVMXC__Order_Status__c = $wo_status;
	workorder.SVMXC__State__c = $wo_state;
	result = sforce.connection.create([workorder]);
		
	var terrRule_WO_List = sforce.connection.query("Select Id, SVMXC__Primary_Territory__c, SVMXC__Order_Type__c, SVMXC__Order_Status__c From SVMXC__Service_Order__c Where Id = '" +result[0].id+"'");
	var terrRule_WO_arr = terrRule_WO_List.getArray("records");
	
	_log(result[0].id)
	
		var terrRule_WO = terrRule_WO_arr[0];
		_log("SVMXC__Order_Type__c = "+terrRule_WO.SVMXC__Order_Type__c+" , SVMXC__Order_Status__c = "+terrRule_WO.SVMXC__Order_Status__c+" , SVMXC__Primary_Territory__c = "+ terrRule_WO.SVMXC__Primary_Territory__c);
		if(terrRule_WO.SVMXC__Order_Type__c === 'Cleaning' & terrRule_WO.SVMXC__Order_Status__c === 'Open' & terrRule_WO.SVMXC__Primary_Territory__c === null)
		{
			_fail("Primary Territory is not auto populated");
		}
		else
		{
			_log("Primary Territory is auto populated");
		}
	

	return result[0].id;
}	
