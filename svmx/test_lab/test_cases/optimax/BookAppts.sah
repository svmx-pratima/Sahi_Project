//<browser>
function login($g_un,$g_pwd){
	
	var $login_result = sforce.connection.login($g_un,$g_pwd);
	$sfdc_sessionId = $login_result.sessionId;
	_log("$sfdc_sessionId = "+$sfdc_sessionId);
	return $login_result;
} 


function get_WO ($dp_Id)
{
		var WO_List = _call(sforce.connection.query("Select Id, Name, SVMXC__Dispatch_Process__c, SVMXC__SM_Appointment_Type__c, SVMXC__Work_Order_Scheduling_Status__c, SVMXC__Appointment_Promised_Time_Slot__c From SVMXC__Service_Order__c Where SVMXC__Dispatch_Process__c = '"+$dp_Id+"' AND SVMXC__Work_Order_Scheduling_Status__c = 'Tentative' AND SVMXC__SM_Appointment_Type__c != null AND SVMXC__Appointment_Promised_Time_Slot__c = null ORDER BY Id DESC limit 1"));
		var WO_arr = WO_List.getArray("records");
		var $number = WO_arr[0].Id;
		_log($number);
		return $number;
}
