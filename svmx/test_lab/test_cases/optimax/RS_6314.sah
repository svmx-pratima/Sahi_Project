//<browser>
function login($g_un,$g_pwd){
	
	var $login_result = sforce.connection.login($g_un,$g_pwd);
	return $login_result;
}

function create_Skill($s_Category, $s_Name)
{
	var skills_List = sforce.connection.query("SELECT Name,SVMXC__Active__c FROM SVMXC__Skill__c Where SVMXC__Skill_Category__c = '"+$s_Category+"' AND Name = '" + $s_Name+"'");
	var skills_arr = skills_List.getArray("records");
	if(skills_arr.length > 0)
		{
		return "Skill Exists";
		}
	else
		{
		var newskill = new sforce.SObject("SVMXC__Skill__c");
		newskill.Name = $s_Name;
		newskill.SVMXC__Active__c = true;
		newskill.SVMXC__Skill_Category__c = $s_Category;
		result = sforce.connection.create([newskill]);
		return result;
		}
	
}

